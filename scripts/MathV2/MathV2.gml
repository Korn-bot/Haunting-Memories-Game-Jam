function Vector2(n_x,n_y) constructor {
	
	_x = n_x;
	_y = n_y;
	
}

function Vector3(n_x,n_y,n_z) constructor {
	
	_x = n_x;
	_y = n_y;
	_z = n_z;
	
}

function Vector4(n_x,n_y,n_z,n_w) constructor {
	
	_x = n_x;
	_y = n_y;
	_z = n_z;
	_w = n_w;
}

function Vector5(n_x,n_y,n_z,n_w,n_a) constructor {
	
	_x = n_x;
	_y = n_y;
	_z = n_z;
	_w = n_w;
	_a = n_a;
}